{
    "version": 2,
    "builds": [
      {
        "src": "*.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/app.py",
        "use": "@vercel/python"
      },
      {
        "src": "/api/(.*)",
        "dest": "/api"
      }
    ],
    "env": {
      "PYTHON_VERSION": "3.9"
    }
  }